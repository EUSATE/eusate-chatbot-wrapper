{
  "name": "@eusate/messenger-sdk",
  "version": "0.1.0",
  "description": "Eusate Messenger SDK - Embeddable AI-powered customer support",
  "main": "dist/eusate-sdk.min.js",
  "module": "dist/eusate-sdk.min.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c --environment NODE_ENV:production",
    "build:dev": "npm run clean && rollup -c --environment NODE_ENV:development",
    "dev": "rollup -c --watch --environment NODE_ENV:development",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "npm run lint -- --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" ",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EUSATE/eusate-messenger-sdk"
  },
  "keywords": [
    "messenger",
    "sdk",
    "customer-support"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EUSATE/eusate-messenger-sdk/issues"
  },
  "homepage": "https://github.com/EUSATE/eusate-messenger-sdk#readme",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "rollup": "^4.34.9",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.45.0"
  }
}
